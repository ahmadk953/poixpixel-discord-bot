{
  "name": "poixpixel-discord-bot",
  "version": "1.0.0",
  "main": "./src/discord-bot.ts",
  "author": "Poixpixel",
  "license": "Apache-2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "compile": "npx tsc",
    "dev": "yarn compile && node ./target/discord-bot.js",
    "no-deploy": "cross-env SKIP_COMMAND_DEPLOY=true yarn run dev",
    "start": "yarn compile && pm2 start ./target/discord-bot.js --name poixpixel-discord-bot",
    "restart": "pm2 restart poixpixel-discord-bot",
    "undeploy-commands": "yarn compile && node --experimental-specifier-resolution=node ./target/util/undeployCommands.js",
    "lint": "npx eslint ./src && npx tsc --noEmit",
    "clean": "rimraf ./target",
    "format": "prettier --check --ignore-path .prettierignore .",
    "format:fix": "prettier --write --ignore-path .prettierignore .",
    "postinstall": "ts-patch install -s && husky"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.94",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.212.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-logs": "^0.212.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "discord.js": "^14.25.1",
    "drizzle-orm": "^0.45.1",
    "ioredis": "^5.9.3",
    "pg": "^8.18.0",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "rimraf": "^6.1.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "typescript-transform-paths": "^3.5.6"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
